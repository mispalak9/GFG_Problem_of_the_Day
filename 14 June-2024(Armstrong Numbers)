#User function Template for python3

class Solution:
    def armstrongNumber (self, n):
        # code here 

        n_str = str(n)

        # Calculate the sum of the cubes of the digits
        sum_of_cubes = sum(int(digit)**3 for digit in n_str)

        # Check if the sum of cubes is equal to n
        if sum_of_cubes == n:
            return "Yes"
        else:
            return "No"
